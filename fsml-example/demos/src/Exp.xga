customize dsls.Exp *.exp

postprocessor "EXP";

File:
	limit + to 1..5;

MulDiv:
	limit * to 1..4;
	
AddSub:
	limit * to 1..4;

Assign:
	replace LOWER by "<var name>";

Ref:
	replace LOWER by "<var reference>";
	
Def:
	replace UPPER by "<pattern name>";

Call:
	replace UPPER by "<pattern reference>";